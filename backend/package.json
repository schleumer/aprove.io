  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "build:prod": "tsc && cp tsconfig.prod.json dist/tsconfig.json",
    "start:prod": "cd dist && node index.js",
    "debug": "nodemon --config ./nodemon.debug.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "db-migrate up --config=database.json",
    "migrate:reset": "db-migrate reset --config=database.json",
    "reset": "npm run migrate:reset && npm run migrate && ts-node --project ./tsconfig.json ./src/seeder.ts",
    "seed": "ts-node --project ./tsconfig.json ./src/seeder.ts",
    "seed:prod": "cd dist && node ./seeder.js",
    "error-tags": "find . -path ./node_modules -prune -o -type f -exec grep -oi '\\[ERR[0-9]\\+\\]' \\{\\} \\; | sort -V | less +G"
  },
