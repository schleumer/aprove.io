    "rimraf": "2.6.3",
    "shelljs": "0.8.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "svg-url-loader": "2.3.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
